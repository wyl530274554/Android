<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <EditText
        android:id="@+id/et_dialog_collect_web_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="请输入标题" />

    <EditText
        android:id="@+id/et_dialog_collect_web_url"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="请输网址" />

    <Button
        android:id="@+id/bt_dialog_collect_web_ok"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="确定" />
</LinearLayout>